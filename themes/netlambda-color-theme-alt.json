{
  "name": "Solarized lambda light",
  "colors": {
    "editor.background": "#FDF6E3",
    "editor.foreground": "#586E75"
    // "list.activeSelectionIconForeground": "#fff"
  },
  "semanticHighlighting": true,
  "semanticTokenColors": {
    // "keyword": "#859900",
    "keyword:rust": {
      // "fontStyle": "",
      // "foreground": "#2aa198"
    },
    "selfKeyword": {
      "foreground": "#AB6526",
      "fontStyle": ""
    },
    // "storage.type": "#002b36",
    "property.declaration": "#657b83",
    "parameter": {
      "foreground": "#CB4B16",
      "fontStyle": "underline"
    },
    "class": {
      // "foreground": "#af8700",
      "foreground": "#268BD2",
      // "foreground": "#6c71c4",
      "fontStyle": "bold"
    },
    "class.defaultLibrary": {
      "foreground": "#6c71c4",
      "fontStyle": "bold"
    },
    "variable.local": {
      "fontStyle": "",
      "foreground": "#268bd2"
    },
    "struct": {
      "fontStyle": ""
    },
    "struct.defaultLibrary": {
      "foreground": "#6c71c4",
      "fontStyle": "bold"
    },
    "enum": {
      // "fontStyle": "underline"
    },
    // "enumMember": "#ff0000",
    "variable": {
      "fontStyle": "",
      // "foreground": "#268bd2"
    },
    "variable.mutable": {
      // "fontStyle": "italic",
      // "foreground": "#586e75"
    }
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#93A1A1"
      }
    },
    {
      "name": "Comments: Preprocessor",
      "scope": "comment.block.preprocessor",
      "settings": {
        "fontStyle": "",
        "foreground": "#93A1A1"
      }
    },
    {
      "name": "Comments: Documentation",
      "scope": ["comment.documentation", "comment.block.documentation"],
      "settings": {
        "foreground": "#448C27"
      }
    },
    {
      "name": "Invalid - Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#660000"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword"],
      "settings": {
        "fontStyle": "",
        "foreground": "#859900"
      }
    },
    {
      "name": "Keywords control flow",
      "scope": ["keyword.control.rust"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#859900"
      }
    },
    {
      "name": "Keywords Other",
      "scope": [
        "keyword.other.rust",
        "storage.type.rust",
        "keyword.declaration.struct.rust"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#002b36"
      }
    },
    {
      "name": "Keywords Other",
      "scope": ["storage.type.rust"],
      "settings": {
        "foreground": "#002b36"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#002b36"
      }
    },
    {
      "name": "Types",
      "scope": ["storage.type"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#002b36"
      }
    },
    {
      "name": "Types build in",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "Language Constants",
      "scope": ["constant.language", "support.constant", "variable.language"],
      "settings": {
        "foreground": "#AB6526"
      }
    },
    // {
    //   "name": "Variables (only import in ts)",
    //   "scope": ["variable", "support.variable"],
    //   "settings": {
    //     "foreground": "#6c71c4"
    //   }
    // },
    {
      "name": "Variables object properties",
      "scope": ["variable.other.constant.property"],
      "settings": {
        "foreground": "#657b83"
      }
    },
    {
      "name": "Variables other",
      "scope": ["variable.other.object", "variable.other.readwrite"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#268bd2"
      }
    },
    {
      "name": "Variable default library",
      "scope": ["support.variable", "defaultLibrary"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#586e75"
      }
    },
    {
      "name": "Variable property",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "fontStyle": "",
        "foreground": "#657b83"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#d33682"
      }
    },
    {
      "name": "Functions keyword",
      "scope": ["meta.function.definition", "keyword.other.fn"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#073642"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class"
      ],
      "settings": {
        // "fontStyle": "bold",
        "foreground": "#859900"
      }
    },
    {
      "name": "Interface",
      "scope": [
        "entity.name.type.interface",
        "entity.other.inherited-class.interface",
        "support.interface"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#b58900"
      }
    },
    // {
    // 	"name": "Type Classes",
    // 	"scope": [
    // 		"entity.name.type",
    // 		// "entity.other.inherited-class",
    // 		// "support.class"
    // 	],
    // 	"settings": {
    // 		"fontStyle": "bold",
    // 		"foreground": "#268BD2"
    // 	}
    // },
    {
      "name": "Exceptions",
      "scope": "entity.name.exception",
      "settings": {
        "foreground": "#CB4B16"
      }
    },
    {
      "name": "Sections",
      "scope": "entity.name.section",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Numbers, Characters",
      "scope": ["constant.numeric", "constant.character", "constant"],
      "settings": {
        "foreground": "#D33682"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#2AA198"
      }
    },
    {
      "name": "Strings: Escape Sequences",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "name": "Strings: Regular Expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#D30102"
      }
    },
    {
      "name": "Strings: Symbols",
      "scope": "constant.other.symbol",
      "settings": {
        "foreground": "#AB6526"
      }
    },
    // {
    // 	"name": "Punctuation",
    // 	"scope": "punctuation",
    // 	"settings": {
    // 		"foreground": "#D30102"
    // 	}
    // },
    {
      "name": "HTML: Doctype Declaration",
      "scope": [
        "meta.tag.sgml.doctype",
        "meta.tag.sgml.doctype string",
        "meta.tag.sgml.doctype entity.name.tag",
        "meta.tag.sgml punctuation.definition.tag.html"
      ],
      "settings": {
        "foreground": "#AAAAAA"
      }
    },
    {
      "name": "HTML: Tags",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end"
      ],
      "settings": {
        "foreground": "#93A1A1"
      }
    },
    {
      "name": "HTML: Tag Names",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#268BD2"
      }
    },
    {
      "name": "HTML: Attribute Names",
      "scope": [
        "meta.tag entity.other.attribute-name",
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#93A1A1"
      }
    },
    {
      "name": "HTML: Entities",
      "scope": ["constant.character.entity", "punctuation.definition.entity"],
      "settings": {
        "foreground": "#AB6526"
      }
    },
    {
      "name": "CSS: Selectors",
      "scope": [
        "meta.selector",
        "meta.selector entity",
        "meta.selector entity punctuation",
        "entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#7A3E9D"
      }
    },
    {
      "name": "CSS: Property Names",
      "scope": ["meta.property-name", "support.type.property-name"],
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "CSS: Property Values",
      "scope": [
        "meta.property-value",
        "meta.property-value constant.other",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#B58900"
      }
    },
    {
      "name": "CSS: Important Keyword",
      "scope": "keyword.other.important",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Markup: Deletion",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup: Error",
      "scope": "markup.error",
      "settings": {
        "foreground": "#660000"
      }
    },
    {
      "name": "Markup: Insertion",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Markup: Link",
      "scope": "meta.link",
      "settings": {
        "foreground": "#4B83CD"
      }
    },
    {
      "name": "Markup: Output",
      "scope": ["markup.output", "markup.raw"],
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "name": "Markup: Prompt",
      "scope": "markup.prompt",
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "name": "Markup: Heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#AA3731"
      }
    },
    {
      "name": "Markup: Strong",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Traceback",
      "scope": "markup.traceback",
      "settings": {
        "foreground": "#660000"
      }
    },
    // {
    //   "name": "Markup: Underline",
    //   "scope": "markup.underline",
    //   "settings": {
    //     "fontStyle": "underline"
    //   }
    // },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#7A3E9D"
      }
    },
    {
      "name": "Markup Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#4B83CD"
      }
    },
    {
      "name": "Markup Styling",
      "scope": ["markup.bold", "markup.italic"],
      "settings": {
        "foreground": "#448C27"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "",
        "foreground": "#AB6526"
      }
    },
    {
      "name": "Extra: Diff Range",
      "scope": ["meta.diff.range", "meta.diff.index", "meta.separator"],
      "settings": {
        "foreground": "#434343"
      }
    },
    {
      "name": "Extra: Diff From",
      "scope": "meta.diff.header.from-file",
      "settings": {
        "foreground": "#434343"
      }
    },
    {
      "name": "Extra: Diff To",
      "scope": "meta.diff.header.to-file",
      "settings": {
        "foreground": "#434343"
      }
    }
  ]
}
